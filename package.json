{
    "name": "vscode-nginx",
    "displayName": "nginx.conf",
    "description": "Syntax highlighter for nginx conf files.",
    "version": "0.0.1",
    "publisher": "shanoor",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "NGINX",
            "aliases": ["NGINX"],
            "extensions": [".conf"],
            "configuration": "./nginx.configuration.json"
        }],
        "grammars": [{
            "language": "NGINX",
            "scopeName": "source.nginx",
            "path": "./syntaxes/nginx.tmLanguage"
        }]
    }
}
